{
  "name": "@w01f-o/react-grid-layout",
  "version": "1.0.1",
  "description": "A grid system for react similar to bootstrap-grid",
  "keywords": [
    "react-grid",
    "grid",
    "react-layout",
    "layout"
  ],
  "author": "w01f",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/w01f-o/react-grid-layout",
  "repository": {
    "type": "git",
    "url": "https://github.com/w01f-o/react-grid-layout.git"
  },
  "bugs": {
    "url": "https://github.com/w01f-o/react-grid-layout/issues"
  },
  "main": "./dist/react-grid-layout.umd.cjs",
  "module": "./dist/react-grid-layout.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/react-grid-layout.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/react-grid-layout.umd.cjs"
      }
    },
    "./css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.5",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^3.8.1",
    "clsx": "^2.1.0",
    "sass": "^1.74.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
